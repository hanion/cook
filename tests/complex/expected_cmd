g++ -Wall -Wextra -Werror -g -c -o main.o -Iinclude -Iimgui -Ilibs/glfw/include -Ilibs/glew/include -Ilibs/glm -Llibs/glfw/lib -Llibs/glew/lib 
g++ -Wall -Wextra -Werror -g -c -o input.o -Iinclude -Iimgui -Ilibs/glfw/include -Ilibs/glew/include -Ilibs/glm -Llibs/glfw/lib -Llibs/glew/lib 
g++ -Wall -Wextra -Werror -g -c -o window.o -Iinclude -Iimgui -Ilibs/glfw/include -Ilibs/glew/include -Ilibs/glm -Llibs/glfw/lib -Llibs/glew/lib 
g++ -Wall -Wextra -Werror -g -c -o renderer.o -Iinclude -Iimgui -Ilibs/glfw/include -Ilibs/glew/include -Ilibs/glm -Llibs/glfw/lib -Llibs/glew/lib 
g++ -Wall -Wextra -Werror -g -c -o imgui.o -Iinclude -Iimgui -Ilibs/glfw/include -Ilibs/glew/include -Ilibs/glm -Llibs/glfw/lib -Llibs/glew/lib 
g++ -Wall -Wextra -Werror -g -o game -Iinclude -Iimgui -Ilibs/glfw/include -Ilibs/glew/include -Ilibs/glm main.o input.o window.o renderer.o imgui.o -Llibs/glfw/lib -Llibs/glew/lib -lglfw -lGLEW -lGL -ldl -lpthread
