g++ -Wall -Wextra -Werror -g -c -o main.o main.cpp -Iinclude -Iimgui -Ilibs/glfw/include -Ilibs/glew/include -Ilibs/glm -Llibs/glfw/lib -Llibs/glew/lib 
g++ -Wall -Wextra -Werror -g -c -o input.o input.cpp -Iinclude -Iimgui -Ilibs/glfw/include -Ilibs/glew/include -Ilibs/glm -Llibs/glfw/lib -Llibs/glew/lib 
g++ -Wall -Wextra -Werror -g -c -o window.o window.cpp -Iinclude -Iimgui -Ilibs/glfw/include -Ilibs/glew/include -Ilibs/glm -Llibs/glfw/lib -Llibs/glew/lib 
g++ -Wall -Wextra -Werror -g -c -o renderer.o renderer.cpp -Iinclude -Iimgui -Ilibs/glfw/include -Ilibs/glew/include -Ilibs/glm -Llibs/glfw/lib -Llibs/glew/lib 
g++ -Wall -Wextra -Werror -g -c -o imgui.o imgui.cpp -Iinclude -Iimgui -Ilibs/glfw/include -Ilibs/glew/include -Ilibs/glm -Llibs/glfw/lib -Llibs/glew/lib 
g++ -Wall -Wextra -Werror -g -o game game.cpp -Iinclude -Iimgui -Ilibs/glfw/include -Ilibs/glew/include -Ilibs/glm main.o input.o window.o renderer.o imgui.o -Llibs/glfw/lib -Llibs/glew/lib -lglfw -lGLEW -lGL -ldl -lpthread
